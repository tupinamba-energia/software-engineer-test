generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model EventRaw {
  id          String   @id @default(uuid()) @db.Uuid
  event_id    String
  type        String
  campaign_id String
  creative_id String?
  source      String
  occurred_at DateTime
  user_id     String?
  metadata    Json?
  inserted_at DateTime @default(now())
  updated_at  DateTime @updatedAt

  @@map("events_raw")
}
